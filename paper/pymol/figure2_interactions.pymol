set cartoon_transparency, 0
set cartoon_oval_length, 1.0
set opaque_background, 0
set cartoon_color, white
set ray_trace_mode, 1
set ray_shadow, 0
set dash_width, 10
set surface_cavity_mode, 2
hide lines
util.cbaw


color manganese, prot* and element c
color cyan, 3THB_lig-to-2OWB_pv.3THB_lig_09 and element c
color green, 2YAC_lig-to-2OWB_pv.2YAC_lig and element c
color cyan, 3THB_lig-to-2OWB_pv.3THB_lig_04 and element c
color green, 2YAC_lig-to-2OWB_pv.2YAC_lig_11 and element c


as cartoon
hide cartoon, resid -128
hide sticks
show sticks, het
show sticks, resid 140+133
hide sticks, element H and not (element N+O extend 1)

set_view (\
    -0.662457287,   -0.651827514,    0.369081378,\
    -0.464637190,   -0.028896645,   -0.885000050,\
     0.587551713,   -0.757778466,   -0.283712804,\
    -0.001710700,    0.003489427,  -40.013114929,\
    -2.737388611,   25.870780945,   68.614334106,\
    31.451366425,   49.475791931,  -20.000000000 )

# All the poses...
enable 2YAC_lig-to-2OWB_pv or 2YAC_lig-to-2OWB_pv.2YAC_lig or 2YAC_lig-to-2OWB_pv.2YAC_lig_11 or prot_2OWB or crystal_2YAC or 3THB_lig-to-2OWB_pv or 3THB_lig-to-2OWB_pv.3THB_lig_09 or 3THB_lig-to-2OWB_pv.2YAC_lig_04

