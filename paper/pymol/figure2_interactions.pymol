disable *; enable 2YAC_lig-to-2OWB_pv or 2YAC_lig-to-2OWB_pv.2YAC_lig or 2YAC_lig-to-2OWB_pv.2OWB_prot
delete dist*
dist 2YAC_lig-to-2OWB_pv.2OWB_prot and 133/n, 2YAC_lig-to-2OWB_pv.2YAC_lig and element n and (133/n expand 4)
dist 2YAC_lig-to-2OWB_pv.2OWB_prot and 133/o, 2YAC_lig-to-2OWB_pv.2YAC_lig and element n and (133/o expand 3.1)
hide labels
ray
